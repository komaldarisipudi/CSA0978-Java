import java.util.*;
class celsius
{
    public static void main(String[] args)
    {
        double F,C,Celsius,Farenhiet;
        Scanner scan=new Scanner(System.in);
        System.out.println("Enter the value of F & C:");
        F=scan.nextInt();
        C=scan.nextInt();
        Celsius=((F-32)*5)/9;
        Farenhiet=(9/5)*C+32;
        System.out.println("Celsius=" +Celsius);
        System.out.println("Farenhiet is " +Farenhiet); 
        
    }
}
