import java.util.Scanner;
class Positive
{
    public static void main(String[] args)
    {
        int n;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the value of n:");
        n=sc.nextInt();
        if(n>=0)
        System.out.println("Positive number");
        else
        System.out.println("Negative number");
    }
}
