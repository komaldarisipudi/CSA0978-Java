import java.util.Scanner;
public class Steps{
  public static void main(String[] args){
    Scanner sc=new Scanner(System.in);
    System.out.print("Enter number: ");
    int n=sc.nextInt();
    int step=0;
    while(n!=0){
      if(n%2==0){
        n=n/2;
        step++;
      }else{
        n-=1;
        step++;
      }
    }
    System.out.println("Step: "+step);
    sc.close();
  }
}
