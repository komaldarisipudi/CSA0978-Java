import java.util.*;
class Add
{
    public static void main(String[] args)
    {
        int a,b,c;
        Scanner scan=new Scanner(System.in);
        System.out.println("Enter the value of a & b:");
        a=scan.nextInt();
        b=scan.nextInt();
        c=a+b;
        System.out.println("sum is " +c);
        
    }
}
