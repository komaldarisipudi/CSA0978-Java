import java.util.Scanner;
class Program{
    public static void main(String[] args){
        int sum=0,i=1,n;
        Scanner sc = new Scanner(System.in);
        System.out.println("n=");
        n=sc.nextInt();
        for(i=1;i<n;i++){
            sum+=i;
            System.out.println("sum of n numbers:" +sum);   
        }
    }
}
